    <PackageReference Include="Dapplo.Addons">
      <Version>1.1.3</Version>
    </PackageReference>
    <PackageReference Include="Dapplo.Ini">
      <Version>0.6.2</Version>
    </PackageReference>
    <PackageReference Include="GitLink">
